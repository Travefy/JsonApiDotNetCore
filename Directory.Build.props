<Project>
  <PropertyGroup>
    <NetCoreAppVersion>net6.0</NetCoreAppVersion>
    <NetStandardVersion>netstandard2.1</NetStandardVersion>
    <AspNetCoreVersion>6.0.*</AspNetCoreVersion>
    <EFCoreVersion>6.0.32</EFCoreVersion>
    <NpgsqlPostgreSQLVersion>6.0.*</NpgsqlPostgreSQLVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <NoWarn>$(NoWarn);1591</NoWarn>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <!-- Test Project Dependencies -->
  <PropertyGroup>
    <XUnitVersion>2.4.1</XUnitVersion>
    <BogusVersion>29.0.1</BogusVersion>
    <MoqVersion>4.13.1</MoqVersion>
  </PropertyGroup>
</Project>
